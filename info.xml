<?xml version="1.0"?>
<extension key="au.com.agileware.civimobileactivitycategory" type="module">
  <file>civimobileactivitycategory</file>
  <name>CiviMobile Activity Category</name>
  <description>Limits the Activities shown in the CiviMobile app to only those Activities which have the Activity Category, "CiviMobile". Adds new Activity Category, "CiviMobile" when Extension is installed.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Agileware</author>
    <email>support@agileware.com.au</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/agileware/au.com.agileware.civimobileactivitycategory</url>
    <url desc="Documentation">https://github.com/agileware/au.com.agileware.civimobileactivitycategory</url>
    <url desc="Support">https://agileware.com.au/contact</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-05-23</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Civimobileactivitycategory</namespace>
    <format>22.05.0</format>
    <angularModule>crmCivimobileactivitycategory</angularModule>
  </civix>
  <mixins>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
  <requires>
  <ext>com.agiliway.civimobileapi</ext>
  </requires>
</extension>
